{
  "name": "native-ai-chat",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "watchman watch-del . && expo start",
    "android": "watchman watch-del . && expo run:android",
    "ios": "watchman watch-del . && expo run:ios",
    "lint": "expo lint",
    "prebuild:android": "expo prebuild --platform android",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "type-check": "tsc --noEmit",
    "watchman:clean": "watchman watch-del .",
    "clean": "rm -rf node_modules/ .expo/ android/ ios/ && rm pnpm-lock.yaml && watchman watch-del .",
    "clean:gradle": "cd android && ./gradlew clean && cd .. && pnpm android"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.25",
    "expo-haptics": "~15.0.7",
    "expo-router": "~6.0.15",
    "expo-speech": "~14.0.7",
    "expo-splash-screen": "^31.0.11",
    "expo-status-bar": "^3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-screens": "4.16.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}